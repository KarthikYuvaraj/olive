<configuration>
   <system.diagnostics>
      <sources>
         <source name="System.ServiceModel" 
            switchValue="All"
            propagateActivity="true">
 	    <listeners>
               <add name="traceListener" 
                type="System.Diagnostics.XmlWriterTraceListener" 
                initializeData= "Traces.svclog" />
            </listeners>
         </source>
      </sources>
   </system.diagnostics>
</configuration>
